<h3>以下の内容で、送信する。</h3>

<%= form_with(model: @newblog, url: newblogs_path, local: true) do |form| %>
  <%= form.hidden_field :title %>
  <%= form.hidden_field :content %>

  <p>タイトル:<%= @newblog.title %></p>
  <p>本文:<%= @newblog.content %></p>
  <%= form.submit  "登録する" %>
  <%= form.submit  "戻る", name: 'back' %>
<% end %>
